<style>
    embed#casinoGame {
        width: 100%;
        height: 100vh !important;
    }

    @@media only screen and (max-width: 479px) and (min-width: 320px) {
        .game_detailCustom {
            margin-top: 55px;
            height: auto;
        }
    }
    .game-details {
        background:black;
         
    }
    @@media(max-width:400px){
        #gamerow{
            height:100vh !important;
        }
    }
    @@media(min-device-width:414px){
        #gamerow{
            height:100vh !important;
            
        }
        .fram {
            margin-top: 5px;
        }
    }
    @@media(min-device-width:540px) {

        .fram {
            margin-top: 25px;
        }
    }
    @@media(min-device-width:768px){
        .fram{
            margin-top:25px;
        }
    }
    body{
        background-color:black;
    }
</style>
<div class="game-details" style="width:100%">
    <div class="container game_detailCustom" style="">
        <div class="row" id="gamerow" style="height: 100vh;">
            @* unsuffficent *@
            @if (ViewBag.DirectUrl != null)
            {
                <embed class="fram" id="casinoGame" allowfullscreen="true" src="@ViewBag.DirectUrl" height="" width="100%" />
            }
            else
            {
                @Html.Raw(((string)(ViewBag.HTMLUrl.Replace("<iframe", "<iframe id='height_custom' height='' width='100%'"))))
            }
        </div>
    </div>
</div>
